<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">#FCBA04</item>
        <item name="colorPrimaryDark">#250001</item>
        <item name="colorAccent">#250001</item>
    </style>


    <style name="CustomButton" parent="Widget.AppCompat.Button.Colored">
        <item name="colorButtonNormal">#250001</item>
        <item name="android:textColor">#F3F3F3</item>
        <item name="background">?android:attr/selectableItemBackground</item>
        <item name="colorControlHighlight">#AE1615</item>
    </style>

    <style name="CutomPanel" parent="Widget.AppCompat.Light.ActionBar.Solid">
        <item name="android:background">@android:color/transparent</item>
        <item name="android:textColor">#250001</item>
    </style>

    <style name="GlobalBackground" parent="Widget.AppCompat.Light.ActionBar.Solid">
        <item name="android:background">#FCBA04</item>
    </style>

    <style name="TitlesStyle" parent="Widget.AppCompat.Light.ActionBar.Solid">
<!--        <item name="android:background">#FCBA04</item>-->
        <item name="android:textColor">#250001</item>

    </style>


</resources>
